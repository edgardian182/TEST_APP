<%= render 'shared/errors', obj: @cellphone %>

<%= form_for @cellphone do |f| %>
  <div>
    <%= f.label :reference %>
    <p><%= f.text_field :reference %></p>
  </div>
  <div>
    <%= f.label :os %>
    <%= f.select :os, ['Android', 'iOS', 'Windows Phone', 'Other'] %>
  </div>
  <div>
    <%= f.label :user_id %>
    <%= f.number_field :user_id %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>
  